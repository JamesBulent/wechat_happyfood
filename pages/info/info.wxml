<!--pages/info/info.wxml-->
<view class="container">
		<view class="category-item">
			<block wx:for="{{phoneInfo}}" wx:key="{{this}}">
				<view class="detail-item">
					<text>{{item.key}}</text>
					<text>{{item.val}}</text>
				</view>
			</block>
		</view>

		<view class="category-item">
			<block wx:for="{{softInfo}}"  wx:key="{{this}}">
				<view class="detail-item">
					<text>{{item.key}}</text>
					<text>{{item.val}}</text>
				</view>
			</block>
		</view>

		<view class="category-item">
			<block wx:for="{{screenInfo}}"  wx:key="{{this}}">
				<view class="detail-item">
					<text>{{item.key}}</text>
					<text>{{item.val}}</text>
				</view>
			</block>
		</view>

</view>
